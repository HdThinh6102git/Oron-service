{
  "name": "service",
  "version": "0.0.0",
  "description": "Shop Order Service",
  "main": "dist/app.js",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "entity:load": "ts-node bin/entity",
    "entity:sync": "typeorm-ts-node-commonjs -d bin/ormconfig.ts schema:sync",
    "migrate:generate": "typeorm-ts-node-commonjs migration:generate -d bin/ormconfig.ts src/migration/migration",
    "migrate:create": "cd ./src/migration && typeorm-ts-node-commonjs migration:create",
    "migrate:up": "typeorm-ts-node-commonjs migration:run -d bin/ormconfig.ts",
    "migrate:down": "typeorm-ts-node-commonjs migration:revert -d bin/ormconfig.ts",
    "build": "nest build",
    "prestart": "npm run lint && npm run build",
    "start": "nest start",
    "start:prod": "nest build && node dist/app.js",
    "start:dev": "nest start --watch",
    "start-micro:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:repl": "nest start --entryFile repl",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.ts -i",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger",
    "seed:roles": "ts-node -r tsconfig-paths/register src/seeds/script/role.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.2",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.2",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/axios": "^2.0.0",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/platform-express": "^9.3.2",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.3.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.4",
    "nestjs-pino": "^3.1.2",
    "passport": "^0.6.0",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "bcrypt": "^5.1.1",
    "@types/bcrypt": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "author": "CatsMiaow",
  "license": "MIT"
}
